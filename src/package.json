      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "express": "^4.17.3",
    "mongodb": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.5.4",
    "moment": "^2.29.1",
    "nodemon": "^2.0.6",
    "uuid": "^7.0.3",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
